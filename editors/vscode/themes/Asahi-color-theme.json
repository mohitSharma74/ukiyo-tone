{
  "name": "Ukiyo-Tone Asahi (Morning Sun)",
  "type": "light",
  "colors": {
    "editor.background": "#F0F4F5",
    "editor.foreground": "#264348",
    "activityBar.background": "#DCE4E8",
    "activityBar.foreground": "#264348",
    "sideBar.background": "#E3E9ED",
    "sideBar.foreground": "#264348",
    "editor.selectionBackground": "#CB40424D",
    "editor.lineHighlightBackground": "#DCE4E866",
    "editorCursor.foreground": "#CB4042",
    "editorGroup.border": "#26434833",
    "sideBar.border": "#26434833",
    "activityBar.border": "#26434833",
    "statusBar.background": "#DCE4E8",
    "statusBar.foreground": "#264348",
    "titleBar.activeBackground": "#DCE4E8",
    "titleBar.activeForeground": "#264348",
    "titleBar.inactiveBackground": "#DCE4E8",
    "titleBar.inactiveForeground": "#26434899",
    "terminal.ansiBlack": "#264348",
    "terminal.ansiRed": "#CB4042",
    "terminal.ansiGreen": "#4F7942",
    "terminal.ansiYellow": "#8C5A3C",
    "terminal.ansiBlue": "#264348",
    "terminal.ansiMagenta": "#CB4042",
    "terminal.ansiCyan": "#4F7942",
    "terminal.ansiWhite": "#F2EBD3"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#A19A84",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage", "storage.type"],
      "settings": {
        "foreground": "#CB4042"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#4F7942"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#264348"
      }
    },
    {
      "scope": ["variable", "variable.parameter"],
      "settings": {
        "foreground": "#8C5A3C"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "variable.other.constant"],
      "settings": {
        "foreground": "#264348",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["support.type.property-name.json", "string.key.json", "entity.name.tag.yaml"],
      "settings": {
        "foreground": "#CB4042"
      }
    },
    {
      "scope": ["markup.heading", "markup.heading.markdown"],
      "settings": {
        "foreground": "#CB4042",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold", "markup.bold.markdown"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic", "markup.italic.markdown"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.raw", "markup.fenced_code.block", "markup.inline.raw"],
      "settings": {
        "foreground": "#4F7942"
      }
    },
    {
      "scope": ["markup.quote", "markup.quote.markdown"],
      "settings": {
        "foreground": "#A19A84",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.underline.link", "string.other.link"],
      "settings": {
        "foreground": "#264348",
        "fontStyle": "underline"
      }
    }
  ]
}

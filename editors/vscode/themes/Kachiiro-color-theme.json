{
  "name": "Ukiyo-Tone Kachi-iro (Victory Color)",
  "type": "dark",
  "colors": {
    "editor.background": "#0D1117",
    "editor.foreground": "#FFFFFF",
    "activityBar.background": "#05070A",
    "activityBar.foreground": "#FFFFFF",
    "sideBar.background": "#090C10",
    "sideBar.foreground": "#FFFFFF",
    "editor.selectionBackground": "#CB40424D",
    "editor.lineHighlightBackground": "#161B22",
    "editorCursor.foreground": "#FF003C",
    "editorGroup.border": "#30363D",
    "sideBar.border": "#30363D",
    "activityBar.border": "#30363D",
    "statusBar.background": "#05070A",
    "statusBar.foreground": "#FFFFFF",
    "titleBar.activeBackground": "#05070A",
    "titleBar.activeForeground": "#FFFFFF",
    "titleBar.inactiveBackground": "#05070A",
    "titleBar.inactiveForeground": "#FFFFFF99",
    "terminal.ansiBlack": "#0D1117",
    "terminal.ansiRed": "#FF003C",
    "terminal.ansiGreen": "#00FFCC",
    "terminal.ansiYellow": "#FFFF00",
    "terminal.ansiBlue": "#FFFF00",
    "terminal.ansiMagenta": "#FF003C",
    "terminal.ansiCyan": "#00FFCC",
    "terminal.ansiWhite": "#FFFFFF"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#8B949E",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage", "storage.type"],
      "settings": {
        "foreground": "#FF003C"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#00FFCC"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#FFFF00"
      }
    },
    {
      "scope": ["variable", "variable.parameter"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "variable.other.constant"],
      "settings": {
        "foreground": "#FF003C",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["support.type.property-name.json", "string.key.json", "entity.name.tag.yaml"],
      "settings": {
        "foreground": "#FF003C"
      }
    },
    {
      "scope": ["markup.heading", "markup.heading.markdown"],
      "settings": {
        "foreground": "#FF003C",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold", "markup.bold.markdown"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic", "markup.italic.markdown"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.raw", "markup.fenced_code.block", "markup.inline.raw"],
      "settings": {
        "foreground": "#00FFCC"
      }
    },
    {
      "scope": ["markup.quote", "markup.quote.markdown"],
      "settings": {
        "foreground": "#8B949E",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.underline.link", "string.other.link"],
      "settings": {
        "foreground": "#FFFF00",
        "fontStyle": "underline"
      }
    }
  ]
}

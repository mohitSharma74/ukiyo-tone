{
  "name": "Ukiyo-Tone Karesansui (Dry Landscape)",
  "type": "light",
  "colors": {
    "editor.background": "#E8EDE6",
    "editor.foreground": "#4A5056",
    "activityBar.background": "#DEE4DC",
    "activityBar.foreground": "#4A5056",
    "sideBar.background": "#DEE4DC",
    "sideBar.foreground": "#4A5056",
    "editor.selectionBackground": "#4A505633",
    "editor.lineHighlightBackground": "#DEE4DC80",
    "editorCursor.foreground": "#C19C4D",
    "editorGroup.border": "#949C964D",
    "sideBar.border": "#949C964D",
    "activityBar.border": "#949C964D",
    "statusBar.background": "#DEE4DC",
    "statusBar.foreground": "#4A5056",
    "titleBar.activeBackground": "#DEE4DC",
    "titleBar.activeForeground": "#4A5056",
    "titleBar.inactiveBackground": "#DEE4DC",
    "titleBar.inactiveForeground": "#4A505699",
    "terminal.ansiBlack": "#4A5056",
    "terminal.ansiRed": "#C19C4D",
    "terminal.ansiGreen": "#6A7B54",
    "terminal.ansiYellow": "#C19C4D",
    "terminal.ansiBlue": "#5B7C8C",
    "terminal.ansiMagenta": "#8C7C96",
    "terminal.ansiCyan": "#5B7C8C",
    "terminal.ansiWhite": "#E8EDE6"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#949C96",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage", "storage.type"],
      "settings": {
        "foreground": "#C19C4D"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#6A7B54"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#5B7C8C"
      }
    },
    {
      "scope": ["variable", "variable.parameter"],
      "settings": {
        "foreground": "#5D5650"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "variable.other.constant"],
      "settings": {
        "foreground": "#4A5056",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["support.type.property-name.json", "string.key.json", "entity.name.tag.yaml"],
      "settings": {
        "foreground": "#C19C4D"
      }
    },
    {
      "scope": ["markup.heading", "markup.heading.markdown"],
      "settings": {
        "foreground": "#C19C4D",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold", "markup.bold.markdown"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic", "markup.italic.markdown"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.raw", "markup.fenced_code.block", "markup.inline.raw"],
      "settings": {
        "foreground": "#6A7B54"
      }
    },
    {
      "scope": ["markup.quote", "markup.quote.markdown"],
      "settings": {
        "foreground": "#949C96",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.underline.link", "string.other.link"],
      "settings": {
        "foreground": "#5B7C8C",
        "fontStyle": "underline"
      }
    }
  ]
}
